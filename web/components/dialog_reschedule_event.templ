package components

templ DialogRescheduleEvent() {
	<dialog id="reschedule-dialog" class="relative z-10" aria-labelledby="reschedule" role="dialog" aria-modal="true">
		<div class="fixed inset-0 bg-gray-500 bg-opacity-75 transition-opacity" aria-hidden="true">
			<div class="fixed inset-0 z-10 w-screen overflow-y-auto">
				<div id="reschedule-form" class="flex min-h-full items-end justify-center p-4 text-center sm:items-center sm:p-0"></div>
			</div>
		</div>
	</dialog>
}
